<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kangzhan.student.HomeFragment.activities.SchoolTrainPlaceActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/home_school_train_place_back"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/colorPrimary"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

    <com.holenzhou.pullrecyclerview.PullRecyclerView
        android:id="@+id/home_school_train_place_recycler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <!---item:home_item_school_train_place_layout-->
    </com.holenzhou.pullrecyclerview.PullRecyclerView>

</LinearLayout>
